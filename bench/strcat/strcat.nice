import ackermann; // reuse toSinglePositiveInt

void main(String[] args){
   var n = toSinglePositiveInt(args);
   let s = "hello\n";
   var buffer = new StringBuffer(32);

   while (n-- > 0) buffer.append(s);

   println(buffer.length); 
}
