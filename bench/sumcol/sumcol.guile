#!/usr/bin/guile \
-e main -s
!#

;;; $Id: sumcol.guile,v 1.1 2004-05-19 18:13:43 bfulgham Exp $
;;; http://www.bagley.org/~doug/shootout/
;;; from Brad Knotwell

(define (main args)
  (let ((sum 0))
    (do ((myInt (read-line) (read-line)))
	((eof-object? myInt) (write-line sum))
      (set! sum (+ sum (string->number myInt))))))
