(module fibo
	(option (set! *genericity* #f))
	(main main))

(define (main args)
  (let ((n (or (and (= (length args) 2) (string->number (cadr args))) 1)))
    (display (fib n))
    (newline)))

(define (fib::int n::int)
  (cond ((< n 2) n)
	(else (+ (fib (- n 2)) (fib (- n 1))))))

