import ackermann; // reuse toSinglePositiveInt

void main(String[] args){
    println( fib( toSinglePositiveInt(args) ) ); 
}

int fib(int n)
   requires n >= 0; // precondition

fib(n) { 
   if (n < 2) return 1; else return fib(n-2) + fib(n-1);
}

