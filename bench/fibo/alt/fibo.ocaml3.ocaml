let rec fib1 a b n =
   if n < 2 then a
   else fib1 (a + b) a (n - 1)

let fib n = fib1 1 1 n

let _ =
  let n =
    try int_of_string Sys.argv.(1)
    with Invalid_argument _ -> 1 in
  Printf.printf "%d\n" (fib n)
