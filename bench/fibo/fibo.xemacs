;(setq max-lisp-eval-depth 8000)
;(setq max-specpdl-size 5000)

(defun fib (n)
  (cond ((< n 2) 1)
	(t (+ (fib (- n 2)) (fib (- n 1))))))

(defun main ()
  (let* ((arg (car command-line-args-left))
	 (num (if arg (string-to-number arg) 1)))
    (princ (format "%d\n" (fib num)))))
