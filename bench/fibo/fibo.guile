#!/usr/bin/guile \
-e main -s
!#
;;; $Id: fibo.guile,v 1.2 2005-03-19 00:32:56 igouy-guest Exp $
;;; http://www.bagley.org/~doug/shootout/

(define (main args)
  (let ((n (or (and (= (length args) 2) (string->number (cadr args))) 1)))
    (display (fib n))
    (newline)))

(define (fib n)
  (cond ((< n 2) n)
	(else (+ (fib (- n 2)) (fib (- n 1))))))

