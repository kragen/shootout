%%% -*- mode: prolog -*-
%%% $Id: fibo.gprolog,v 1.1 2004-06-24 07:38:07 bfulgham Exp $
%%% http://shootout.alioth.debian.org/

fibo(0,1).
fibo(1,1).
fibo(N,Val) :- N > 0, N1 is N - 1, fibo(N1,Val1),
		N2 is N - 2, fibo(N2,Val2),
		Val is Val1 + Val2.

main :-
	argument_value(1, ACount),
	number_atom(Count, ACount),
	fibo(Count,Val),
	write(Val), nl, halt.

:- initialization(main).
