            <span class="kwa">if</span> <span class="sym">(</span>neighbour <span class="sym">!=</span> null <span class="sym">&amp;&amp;</span> neighbour<span class="sym">.</span>isEmpty<span class="sym">)</span>
               count <span class="sym">=</span> count <span class="sym">+</span> neighbour<span class="sym">.</span>contiguousEmptyCells

         count <span class="sym">}</span> <span class="kwa">else</span> <span class="sym">{</span> <span class="num">0</span> <span class="sym">}</span>
   <span class="sym">}</span>
