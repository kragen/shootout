<span class="kwa">local</span> len<span class="sym">,</span> sub<span class="sym">,</span> concat <span class="sym">=</span> <span class="kwb">string</span><span class="sym">.</span>len<span class="sym">,</span> <span class="kwb">string</span><span class="sym">.</span>sub<span class="sym">,</span> <span class="kwb">table</span><span class="sym">.</span>concat
