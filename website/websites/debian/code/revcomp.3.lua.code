    <span class="kwa">for</span> i<span class="sym">=</span><span class="num">1</span><span class="sym">,</span><span class="kwd">len</span><span class="sym">(</span>line<span class="sym">),</span><span class="num">4</span> <span class="kwa">do</span> nf <span class="sym">=</span> nf <span class="sym">-</span> <span class="num">1</span><span class="sym">;</span> f<span class="sym">[</span>nf<span class="sym">] =</span> iubc<span class="sym">[</span><span class="kwd">sub</span><span class="sym">(</span>line<span class="sym">,</span> i<span class="sym">,</span> i<span class="sym">+</span><span class="num">3</span><span class="sym">)]</span> <span class="kwa">end</span>
    <span class="kwa">if</span> nf <span class="sym">&lt;=</span> <span class="num">30</span> <span class="kwa">then</span>
      np <span class="sym">=</span> np <span class="sym">+</span> <span class="num">1</span><span class="sym">;</span> p<span class="sym">[</span>np<span class="sym">] =</span> <span class="kwd">concat</span><span class="sym">(</span>f<span class="sym">,</span> <span class="str">&quot;&quot;</span><span class="sym">,</span> nf<span class="sym">,</span> <span class="num">1630</span><span class="sym">);</span> nf <span class="sym">=</span> <span class="num">1631</span>
    <span class="kwa">end</span>
