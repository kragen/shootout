<span class="kwa">function</span> <span class="kwd">InitIntMatrix</span><span class="sym">(</span>a<span class="sym">,</span>b<span class="sym">,</span>c<span class="sym">,</span>d<span class="sym">:</span> <span class="kwb">integer</span><span class="sym">):</span> TIntMatrix<span class="sym">;</span> <span class="kwa">inline</span><span class="sym">;</span>
