    p<span class="sym">[</span><span class="num">1</span><span class="sym">,</span><span class="num">2</span><span class="sym">] := -</span><span class="num">10</span> <span class="sym">*</span> digit<span class="sym">;</span>
    z <span class="sym">:=</span> p <span class="sym">*</span> z<span class="sym">;</span>
