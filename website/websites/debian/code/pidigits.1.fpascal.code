<span class="kwa">end</span><span class="sym">;</span>

<span class="com">{ Matrix manipulation }</span>

<span class="kwa">procedure</span> <span class="kwd">FreeBigIntMatrix</span><span class="sym">(</span>a<span class="sym">:</span> TBigMatrix<span class="sym">);</span> <span class="kwa">inline</span><span class="sym">;</span>
