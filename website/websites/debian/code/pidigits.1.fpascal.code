<span class="com">{ calculating pidigits}</span>

<span class="kwa">procedure</span> <span class="kwd">PrintPiDigits</span><span class="sym">(</span><span class="kwa">const</span> NumDigits<span class="sym">:</span> <span class="kwb">integer</span><span class="sym">);</span>
<span class="kwa">var</span>
  n<span class="sym">:</span> <span class="kwb">integer</span> <span class="sym">=</span> <span class="num">0</span><span class="sym">;</span>
  k<span class="sym">:</span> <span class="kwb">integer</span> <span class="sym">=</span> <span class="num">0</span><span class="sym">;</span>
  z<span class="sym">:</span> TBigMatrix<span class="sym">;</span>
  x<span class="sym">,</span>p<span class="sym">:</span> TIntMatrix<span class="sym">;</span>
