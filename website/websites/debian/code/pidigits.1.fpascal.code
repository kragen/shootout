operator <span class="sym">* (</span>a<span class="sym">:</span> TBigMatrix<span class="sym">;</span> b <span class="sym">:</span> TIntMatrix<span class="sym">) :</span> TBigMatrix<span class="sym">;</span>
<span class="kwa">begin</span>
