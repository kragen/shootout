<span class="kwa">function</span> <span class="kwd">DotProduct</span><span class="sym">(</span>a1<span class="sym">,</span>a2<span class="sym">:</span> TBigInt<span class="sym">;</span> b1<span class="sym">,</span>b2<span class="sym">:</span> <span class="kwb">integer</span><span class="sym">;</span> FreeBigInt<span class="sym">:</span> <span class="kwb">boolean</span><span class="sym">) :</span> TBigInt<span class="sym">;</span> <span class="kwa">inline</span><span class="sym">;</span>
