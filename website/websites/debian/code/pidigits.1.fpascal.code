  p <span class="sym">:=</span> <span class="kwd">InitIntMatrix</span><span class="sym">(</span><span class="num">10</span><span class="sym">,</span> <span class="num">0</span><span class="sym">,</span> <span class="num">0</span><span class="sym">,</span> <span class="num">1</span><span class="sym">);</span>
  x<span class="sym">[</span><span class="num">2</span><span class="sym">,</span><span class="num">1</span><span class="sym">] :=</span> <span class="num">0</span><span class="sym">;</span>
  <span class="kwa">while</span> <span class="sym">(</span>n<span class="sym">&lt;</span>NumDigits<span class="sym">)</span> <span class="kwa">do begin</span>
