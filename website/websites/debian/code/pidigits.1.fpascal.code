    BigIntAdd<span class="sym">;</span>
    <span class="kwd">PushBigIntByValue</span><span class="sym">(</span>z<span class="sym">[</span><span class="num">2</span><span class="sym">,</span><span class="num">1</span><span class="sym">]);</span>
    <span class="kwd">BigIntMulInt</span><span class="sym">(</span>x<span class="sym">);</span>
    <span class="kwd">PushBigIntByValue</span><span class="sym">(</span>z<span class="sym">[</span><span class="num">2</span><span class="sym">,</span><span class="num">2</span><span class="sym">]);</span>
    BigIntAdd<span class="sym">;</span>
