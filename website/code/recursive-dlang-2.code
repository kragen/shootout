    <span class="kwa">return</span> <span class="kwb">A</span>ck<span class="sym">(</span>m<span class="sym">-</span><span class="num">1</span><span class="sym">,</span> <span class="kwb">A</span>ck<span class="sym">(</span>m<span class="sym">,</span> n<span class="sym">-</span><span class="num">1</span><span class="sym">));</span>
<span class="sym">}</span>

<span class="kwb">T F</span>ib<span class="sym">(</span><span class="kwb">T</span><span class="sym">)(</span><span class="kwb">T</span> n<span class="sym">)</span>
