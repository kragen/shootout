    <span class="kwd">writefln</span><span class="sym">(</span><span class="str">&quot;Tak(3.0,2.0,1.0): %.1f&quot;</span><span class="sym">,</span><span class="kwb">T</span>ak<span class="sym">(</span><span class="num">3.0</span><span class="sym">,</span><span class="num">2.0</span><span class="sym">,</span><span class="num">1.0</span><span class="sym">));</span>
<span class="sym">}</span>

<span class="kwb">T A</span>ck<span class="sym">(</span><span class="kwb">T</span><span class="sym">)(</span><span class="kwb">T</span> m<span class="sym">,</span> <span class="kwb">T</span> n<span class="sym">)</span>
<span class="sym">{</span>
