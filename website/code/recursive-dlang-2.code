    <span class="kwd">writefln</span><span class="sym">(</span><span class="str">&quot;Fib(%.1f): %.1f&quot;</span><span class="sym">,</span><span class="num">28.0</span><span class="sym">+</span>n<span class="sym">,</span><span class="kwb">F</span>ib<span class="sym">(</span><span class="num">28.0</span><span class="sym">+</span>n<span class="sym">));</span>
    <span class="kwd">writefln</span><span class="sym">(</span><span class="str">&quot;Tak(%d,%d,%d): %d&quot;</span><span class="sym">,</span><span class="num">3</span><span class="sym">*</span>n<span class="sym">,</span><span class="num">2</span><span class="sym">*</span>n<span class="sym">,</span>n<span class="sym">,</span><span class="kwb">T</span>ak<span class="sym">(</span><span class="num">3</span><span class="sym">*</span>n<span class="sym">,</span><span class="num">2</span><span class="sym">*</span>n<span class="sym">,</span>n<span class="sym">));</span>
